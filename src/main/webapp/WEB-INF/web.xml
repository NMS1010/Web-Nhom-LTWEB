<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>SurveyServlet</servlet-name>
    <servlet-class>controllers.assignment1.SurveyServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EmailJoinServlet</servlet-name>
    <servlet-class>controllers.assignment0.EmailJoinServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SurveyServlet</servlet-name>
    <url-pattern>/survey</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EmailJoinServlet</servlet-name>
    <url-pattern>/emaillist</url-pattern>
  </servlet-mapping>
  <error-page>
    <error-code>404</error-code>
    <location>/views/assignment_2/error_404.jsp</location>
  </error-page>
  <error-page>
    <exception-type>java.lang.Throwable</exception-type>
    <location>/views/assignment_2/error_java.jsp</location>
  </error-page>
</web-app>
